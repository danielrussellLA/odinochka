{
    "name": "odinochka",
    "version": "0.0.1",
    "description": "Open source tab vault",
    "permissions": ["contextMenus", "tabs", "unlimitedStorage", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
	"commands": {
	  "odinochka_show": {
		"suggested_key": {
		  "default": "Alt+Shift+O"
		},
		"description": "Show Odinochka"
	  },
	  "odinochka_save_tab": {
		"suggested_key": {
		  "default": "Alt+T"
		},
		"description": "Save Tab"
	  } ,
	  "odinochka_save_win": {
		"suggested_key": {
		  "default": "Alt+O"
		},
		"description": "Save Window"
	  }


	},
    "browser_action": { },
    "manifest_version":2
}
